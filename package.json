{
  "name": "vrite",
  "version": "2023.5.9",
  "license": "AGPL-3.0-only",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "start": "dotenv -- turbo run start",
    "dev": "dotenv -- turbo run dev",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "build:api": "pnpm run build --filter @vrite/api --force",
    "build:app": "pnpm run build --filter @vrite/app --force",
    "build:collab": "pnpm run build --filter @vrite/collaboration --force",
    "start:api": "node dist/api/index.js",
    "start:app": "node dist/app/index.js",
    "start:collab": "node dist/collaboration/index.js"
  },
  "devDependencies": {
    "prettier": "^2.8.8",
    "prettier-plugin-astro": "^0.8.1",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "dotenv-cli": "^7.1.0"
  },
  "packageManager": "pnpm@7.26.0"
}
